CREATE TABLE user (
  user_id CHAR(8) PRIMARY KEY ,
  user_name VARCHAR(10) NOT NULL,
  mail VARCHAR(255) NOT NULL,
  password VARCHAR(128) NOT NULL,
  profile_image BLOB,
  bio VARCHAR(255),
  user_like INT DEFAULT 0 NOT NULL,
  rank INT DEFAULT 0 NOT NULL
);



